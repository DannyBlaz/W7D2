<h1>A User Session Page</h1>

<form action="<%= session_url %>" method = "post">
    <%= auth_token %>

    <label>Email
        <input type="text" name="user[email]" value="email">
    </label>

    <label>Password
        <input type="password" name="user[password]" value="password">
    </label>
    <br>
    <button>Login</button>
</form>